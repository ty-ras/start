{
  "compilerOptions": {
    "strict": true,
    "module": "ESNext",
    "target": "ES2022",
    "moduleResolution": "Node",
    "lib": [
      "ES2022"
    ],
    "sourceMap": false,
    "allowJs": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "exactOptionalPropertyTypes": false,
    "declaration": false,
    "noEmit": true,
    "noErrorTruncation": true,
    "incremental": true,
  }
}