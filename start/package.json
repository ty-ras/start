  "scripts": {
    "build:run": "yarn run lint && yarn run tsc && yarn run chmodx",
    "build:ci": "yarn run clear-build-artifacts && yarn run tsc && yarn run format-output-files && yarn run chmodx",
    "chmodx": "chmod +x dist/cli.mjs",
    "clear-build-artifacts": "rm -rf dist build",
    "format-output-files": "eslint --no-eslintrc --config '.eslintrc.output.cjs' --fix 'dist/*js'",
    "lint": "eslint ./src --ext .ts,.mts,.tsx",
    "dev": "ts-node src/cli.mts",
    "tsc": "tsc --project tsconfig.build.json",
    "tsc:plain": "tsc",
    "test:coverage": "c8 --temp-directory /tmp ava",
    "test:run": "c8 --temp-directory /tmp --reporter text ava"
  },
  "resolutions": {
    "@types/node": "18.15.0",
    "@effect/schema": "0.16.0"
  }
}