    "@types/fs-extra": "11.0.1",
    "@types/gradient-string": "1.1.2",
    "@types/inquirer": "9.0.3",
    "@types/node": "18.15.0",
    "ava": "5.2.0",
    "c8": "7.13.0",
    "eslint": "8.35.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-sonarjs": "0.18.0",
    "prettier": "2.8.4",
