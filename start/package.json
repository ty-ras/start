    "@ty-ras-extras/frontend-io-ts": "0.13.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "@types/react": "18.0.34",
    "@types/react-dom": "18.0.11",
    "@vitejs/plugin-react": "3.1.0",
